Simes <- function(pvals, alpha = 0.05) {
pvals <- sort(pvals)
n <- length(pvals)
return(
sum(sapply(seq_along(pvals), function(j) ifelse(pvals[j] < j*alpha/n, 1, 0))) > 0
)
}
